for i in tst:
    if(i > 0):
        print(i)
print('№11')
tst = [7, 1, 2, 5, 3, 9]
tst1 = []
for i in tst:
    if(i > 2 and i < 5):
        tst1 += str(i)
print(tst1)
print('№12')
tst = (1, 2, 3, 4, 5, 6, 7)
res = 0
for i in tst:
    if(i % 2 == 0):
        res += i
print(res)
