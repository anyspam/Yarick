print('№7')
res = 0
tst = [1, 2, 3, 4, 5]
for i in tst:
    res += (i * i)
    print(res)
print('№8')
tst = ['a', 'b', 'c', 'd', 'e']
res = ''
for i in tst:
    res += i
print(res)
