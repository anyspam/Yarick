tst1 = []
tst = str(tst)
for i in tst:
    if(int(i) % 2 != 0):
        tst1 += i
print(tst1)
print('№14')
tst = {1, 3, 6, 7, -9, 12}
for i in tst:
    if i < 0:
        break
    print(i)
print('№15')
res = 0
tst = [7, 1, 2, 5, 0, 3, 9]
for i in tst:
    if(i == 0):
        break
    else:
        res += i
print(res)
print('№16')
tst = 897654
tst = str(tst)
tst1 = []
for i in tst:
    tst1 += i
    if i == '6':
        break
print(tst1)
print('№17')
tst = {'a', 'b', 'c', 'd', 'e'}
for i in tst:
    if(i == 'd'):
        continue
    print(i)
print('№18')
tst = [6, 3, -2, 8, -4, 9]
for i in tst:
    if i < 0:
        continue
    print(i)
print('№19')
tst = ['a', 'b', 'c', 'd', 'e']
for i in tst:
    if i == 'b':
        continue
    print(i)
print("№20")
tst = [8, 6, -4, 2, -1]
for i, value in enumerate(tst):
    if(value < 0):
        break
    print(i, value)
