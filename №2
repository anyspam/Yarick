print ('Задание 1')
tst = {
	'a': 1,
	'b': 2,
	'c': 3,
	'd': 4,
	'e': 5
}
for key in tst:
	print(key)

print ('Задание 2')
tst = {
	2: 'a',
	4: 'b',
	6: 'c',
	8: 'd'
}
for key in tst.keys():
	if key != 8:
		print(key)

print ('Задание 3')
tst = {
	'1': 'a',
	'2': 'b',
	'3': 'c',
	'4': 'd'
}
keys_turple = tuple(key for key in tst.keys() if key in{'2','3','4'})
print(keys_turple)

print ('Задание 4')
tst = {
	'a': 1,
	'b': 2,
	'c': 3,
	'd': 4,
	'e': 5
}
for el in tst.values():
	print(el)
	
print ('Задание 5')
tst = {
	'a': 1,
	'b': 2,
	'c': 3,
	'd': 4,
	'e': 5
}
total_sum = 0
for value in tst.values():
  total_sum += value
print(total_sum)

print ('Задание 6')
tst = {
	'1': 'a',
	'2': 'b',
	'3': 'c',
	'4': 'd'
}
res = ''
for value in tst.values():
	 res += value
print(res)

print ('Задание 7')
dct = {
	'a': 1,
	'b': 2,
	'c': 3,
	'd': 4,
	'e': 5
}
for el in dct.items():
	print(el)

print ('Задание 8')
months = {
	1: 'January', 
	2: 'February',
	3: 'March', 
	4: 'April', 
	5: 'May', 
	6: 'June',
    7: 'July',
	8: 'August', 
	9: 'September', 
	10: 'October', 
	11: 'November', 
	12: 'December'
}
for item in enumerate(months.items(), 1):
    print(item)

