print('Задание 12')
for num in range(1, 11):
	print(num)

print('Задание 13')
for num in range(20, 9, -1):
    print(num)

print('Задание 14')
num = [num for num in range(1, 6)]
print(num)

print('Задание 15')
for num in range(2, 101, 2):
    print(num)

print('Задание 16')
for num in range(-10, 11):
      print(num)

print('Задание 17')
for num in range(20, 1, -3):
    print(num)

print('Задание 18')

tst1 = [1, 3, 5]
tst2 = [2, 4, 6]
for el in zip(tst1, tst2):
	print(el)

print('Задание 19')

tst1 = ['a', 'b', 'c']
tst2 = ['d', 'e', 'f']
res = []
for index, (el1, el2) in enumerate(zip(tst1, tst2), 1):
    res.append(el1)
    res.append(str(index))
    res.append(el2)
print(res)

print('Задание 20')
tst1 = [11, 12, 13, 14]
tst2 = [21, 22, 23, 24]
tst3 = [31, 32, 33, 34]
res = []
for el1, el2, el3 in zip(tst1, tst2, tst3):
    res.append(el1 + el2 + el3)
print(res)
